<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <data>
        <!-- cdn.dokter tree view -->
        <record id="cdn_dokter_view_tree" model="ir.ui.view">
            <field name="name">cdn.dokter.view.tree</field>
            <field name="model">cdn.dokter</field>
            <field name="arch" type="xml">
                <tree>
                    <!-- Add your fields here -->
                    <field name="name"/>
                    <field name="street"/>
                    <field name="city"/>
                    <field name="mobile"/>
                    <field name="email"/>
                    <field name="spesialis_ids"/>
                </tree>
            </field>
        </record>

        <!-- cdn.dokter form view -->
        <record id="cdn_dokter_view_form" model="ir.ui.view">
            <field name="name">cdn.dokter.view.form</field>
            <field name="model">cdn.dokter</field>
            <field name="arch" type="xml">
                <form string="">
                    <sheet>
                        <group>
                            <!-- Add your fields here -->
                            <field name="name"/>
                            <field name="street"/>
                            <field name="city"/>
                            <field name="mobile"/>
                            <field name="email"/>
                            <field name="spesialis_ids" widget="many2many_tags"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <!-- cdn.dokter action window -->
        <record id="cdn_dokter_action" model="ir.actions.act_window">
            <field name="name">Doctor List</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">cdn.dokter</field>
            <field name="view_mode">tree,form</field>
            <field name="help" type="html">
                <p class="oe_view_nocontent_create">
                    <!-- Add Text Here -->
                </p><p>
                    <!-- More details about what a user can do with this object will be OK --> 
                
                </p>
            </field>
        </record>
        
        <!-- This Menu Item must have a parent and an action -->
        <menuitem id="dokter_menu_act" name="Doctor List" parent="dokter_menu_categ" action="cdn_dokter_action" sequence="1"/>
    
    </data>
    
</odoo>